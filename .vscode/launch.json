    "version": "0.2.0",
    "configurations": [
        {
            "name": "genie-grammar",
            "type": "antlr-debug",
            "request": "launch",
            "input": "${file}",
            "grammar": "${workspaceFolder}/grammar/GenieParser.g4",
            "startRule": "featureFile",
            "printParseTree": true,
            "visualParseTree": true
        },
        {
            "name": "genie-fragment",
            "type": "antlr-debug",
            "request": "launch",
            "input": "${file}",
            "grammar": "${workspaceFolder}/grammar/GenieParser.g4",
            "startRule": "fragmentsFile",
            "printParseTree": true,
            "visualParseTree": true
        }
    ]
}